{
  "name": "baseball-theater-v2",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.ts",
  "scripts": {
    "installall": "yarn install --check-files && cd client && yarn install --check-files",
    "client": "cd client && yarn start",
    "server": "node -r ts-node/register --inspect=5001 --nolazy server/server.ts --runInBand",
    "dev": "concurrently --kill-others \"yarn server\" \"yarn client\"",
    "clean": "if exist output rmdir /s /q output",
    "build-client": "cd client && yarn build",
    "build-server": "node server/scripts/build.js",
    "build-populator": "node workers/scripts/build_populator.js",
    "build": "yarn clean && yarn build-client && yarn build-server && yarn build-populator",
    "run-server": "cd output && node server/server.js",
    "finalize": "node server/scripts/finalize.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "baseball-theater-engine": "file:./baseball-theater-engine/dist",
    "client-oauth2": "^4.2.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cross-fetch": "^3.0.4",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "apicache": "^1.5.3",
    "express-serve-static-core": "^0.1.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongodb": "^3.3.2",
    "simple-oauth2": "^3.1.0",
    "ts-node": "^8.3.0",
    "typescript": "3.7.2",
    "uuid-apikey": "^1.4.4",
    "x2js": "^3.3.1"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/apicache": "^1.2.2",
    "@types/express": "^4.17.1",
    "@types/mongodb": "^3.3.3",
    "@types/serve-static": "^1.13.3",
    "@types/simple-oauth2": "^2.2.1",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-react": "^3.23.0",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "concurrently": "^4.0.1",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "fs-extra": "^8.1.0",
    "gar": "^1.0.4",
    "patreon": "^0.4.1",
    "serve-static": "^1.14.1",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-react": "^3.23.11",
    "ts-loader": "^6.1.0",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.8"
  }
}
